  MTWi : SUBROUTINE
    BEAM, ENERGY=Ei
    USE, DL10
    VALUE, BXi,AXi,BYi,AYi
    MATCH, BETA0=TWi
      VARY, BXi, STEP=1.E-6
      VARY, AXi, STEP=1.E-6
      VARY, BYi, STEP=1.E-6
      VARY, AYi, STEP=1.E-6
      CONSTR, MRK0F, BETX=BX10, ALFX=AX10, BETY=BY10, ALFY=AY10
     !LMDIF, TOL=1.E-20
     !MIGRAD, TOL=1.E-20
    ENDMATCH
    VALUE, BXi,AXi,BYi,AYi
    BEAM, ENERGY=Ei
    USE, DL10
    PRINT, FULL
    TWISS, BETA0=TWi, SAVE
    PLOT, TABLE=TWISS, HAXIS=S, VAXIS=BETX,BETY, &
      STYLE=100, SPLINE, RANGE=#S/MRK0F, FILE="match"
    PLOT, TABLE=TWISS, HAXIS=S, VAXIS=DX,DY, &
      STYLE=100, SPLINE, RANGE=#S/MRK0F, FILE="match"
    PLOT, TABLE=TWISS, HAXIS=S, VAXIS=BETX,BETY, &
      STYLE=100, SPLINE, FILE="match"
